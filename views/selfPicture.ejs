
<div class="sPicture">
  <h3>记录每一张微博照片</h3>
  <% var lastday = 0 %>
  <% posts.forEach(function (post,index) {%>
  <% if(post.picture){%>
  <% if(lastday != post.time.day ) {%>
  <h3><%= post.time.day %></h3>
  <% lastday = post.time.day }%>
  <a class="pictureGrop" href="javascript:;"><img src="/images/picture/<%= post.picture%>" alt=""></a>
  <%}%>
  <%})%>
</div>
